<!-- Your form goes here! -->
<%= form_for(@baby) do |f| %>
  <%= f.label :first_name %>
  <%= f.text_field :first_name %><br>

  <%= f.label :last_name %>
  <%= f.text_field :last_name %><br>

  <%= f.label :weight %>
  <%= f.number_field :weight %><br>

  <%= f.label :birth_date %>
  <%= f.text_field :birth_date %><br>

  <%= f.submit %>
<% end %>
